@model SIPA.Models.Domicilio

@{
    ViewData["Title"] = "Delete";
}

<h2>Eliminar</h2>

<h3>¿Desea eliminar @Html.DisplayFor(model => model.Transporte.TransporteID)?</h3>
<div>
    <h4>Domicilio</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Transporte)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Transporte.TransporteID)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.HoraFecha)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.HoraFecha)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="DomicilioID" />
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a asp-action="Index" class="btn btn-danger">Volver</a>
    </form>
</div>
