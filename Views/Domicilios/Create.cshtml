@model SIPA.Models.Domicilio

@{
    ViewData["Title"] = "Create";
}

<h2>Nuevo</h2>

<h4>Domicilio</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="TransporteID" class="control-label"></label>
                <select asp-for="TransporteID" class ="form-control" asp-items="ViewBag.TransporteID"></select>
            </div>
            <div class="form-group">
                <label asp-for="HoraFecha" class="control-label"></label>
                <input asp-for="HoraFecha" class="form-control" />
                <span asp-validation-for="HoraFecha" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Guardar" class="btn btn-info" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index"  class="btn btn-danger">Volver</a>
</div>

